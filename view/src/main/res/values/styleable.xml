<?xml version="1.0" encoding="utf-8"?>
<resources>  
    <declare-styleable name="PulsatorStyleable">
        <attr name="ps_count" format="integer" />
        <attr name="ps_duration" format="integer" />
        <attr name="ps_stroke_width" format="integer" />
        <attr name="ps_style" format="integer" />
        <attr name="ps_color" format="color" />
        <attr name="ps_repeat" format="integer" />
        <attr name="ps_maxScale" format="float" />
        <attr name="ps_startFromScratch" format="boolean" />
        <attr name="ps_interpolator" format="enum">
            <enum name="Linear" value="0" />
            <enum name="Accelerate" value="1" />
            <enum name="Decelerate" value="2" />
            <enum name="AccelerateDecelerate" value="3" />
        </attr>
    </declare-styleable>
</resources>